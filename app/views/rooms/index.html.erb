<div class="container text-center">
  <div class="row">
    <h2 class="mt-5">DM一覧</h2>
    <div class="col-md-10 col-lg-5 mx-auto my-5">
      <% @anotherrooms.each do |e| %>
        <h5 class="table-bordered p-3 mb-2">
          <%= link_to room_path(e.room.id) do %>
            <%= e.user.nickname %>さんのDMへ<br>
          <% end %>
        </h5>
      <% end %>
    </div>
  </div>
</div>